<h1>reminders index</h1>
<h3> <%= @user.user_name.capitalize %>'s Reminders</h3><br />
      <% @user.reminders.each do |reminder| %>
        <blockquote>
          <p><%= reminder.title %></p>
          <p><%= reminder.description %></p>
          <small><cite title="Source Title"><%= reminder.priority %> priority</cite></small>
        </blockquote>
      <% end %>
